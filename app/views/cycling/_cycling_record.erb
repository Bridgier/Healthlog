<div>

  <div class='cycling_record_distance'>
  <fieldset>
    <legend>Info:</legend>
    <table><tr><td>    
    <div class='form_row'>
       <label>Bike:</label>
       <select>
	  <option>Tricross</option>
	  <option>Crossroad</option>
       </select>    
    </div>
    
    <% odomId = 'test' %>

    <div class='form_row'>
        <label >Use Odometer</label><input type='checkbox' onChange="toggleUseOdometer('<%=odomId%>');"/>
    </div>

    <div id="<%=odomId%>-odom" style="display:none">

    <div class='form_row'>
      <label>Prev. Odometer:</label><input type='text'/>
    </div>

    <div class='form_row'>
      <label>New Odometer:</label><input type='text'/>
    </div>
    </div>
    <div class='form_row'>
      <label>Distance:</label><input id='<%=odomId%>-dist' type='text'/>
    </div>
    </td>
    <td valign='top'>
    <div class='cycling_record_notes'>
    <div>Notes:</div>
    <textarea></textarea>
    </div>
    </td></tr></table>
  </fieldset>  
  </div>
</div>